<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="320.0" prefWidth="307.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="javaCode.user.UpdateInfoController">
    <children>
        <TextField fx:id="txtChangePhone" layoutX="73.0" layoutY="73.0"/>
        <TextField fx:id="txtChangeEmail" layoutX="73.0" layoutY="128.0"/>
        <TextField fx:id="txtChangePassword" layoutX="73.0" layoutY="182.0"/>
        <Label layoutX="73.0" layoutY="49.0" prefHeight="17.0" prefWidth="154.0" text="Change phonenumber"/>
        <Label layoutX="73.0" layoutY="111.0" prefHeight="17.0" prefWidth="89.0" text="Change email"/>
        <Label layoutX="73.0" layoutY="160.0" prefHeight="17.0" prefWidth="167.0" text="Change password"/>
        <Button fx:id="btnChange" layoutX="181.0" layoutY="247.0" mnemonicParsing="false" onAction="#changeInfo"
                text="Change"/>
        <Button fx:id="btnCancel" layoutX="73.0" layoutY="247.0" mnemonicParsing="false" onAction="#cancel"
                text="Cancel"/>
    </children>
</AnchorPane>
